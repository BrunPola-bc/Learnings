          <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="4.5.2">
            <name>Counter fetch and update</name>
            <sequenceNumber>0</sequenceNumber>
            <enabled>true</enabled>
            <script>var dbConn;

try {
    dbConn = getConnection();
    logger.info(&quot;WRITER TRANSFORMER GOT CONNECTION!&quot;);

    var query = 	&quot;UPDATE counters &quot; 
			+	&quot;SET value = value + 1 &quot; 
			+	&quot;WHERE name = &apos;db_writer_channel_counter&apos; &quot; 
			+	&quot;RETURNING value AS cnt;&quot;;


    // Atomically increment the counter and get the new value
    var rs = dbConn.executeCachedQuery(query);
    
    if (!rs.next()) {
        throw &quot;Counter row not found: db_writer_channel_counter&quot;;
    }
    var cnt = rs.getInt(&apos;cnt&apos;);

    // Store it in the channel map for downstream transformers
    channelMap.put(&apos;cnt&apos;, cnt);

} finally {
    if (dbConn) dbConn.close();
}</script>
          </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
